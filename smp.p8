pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
-- smp 
-- whitney trovinger

x = 60 
y = 64

roomx = 128
roomy = 128 

tablex = {}
tabley = {}

t = true
f = false 

-- populates a table based on
-- the size of a room  
function popnums()

	for i=0, roomx, 8 do
		add(tablex, i)
	end
	
	for j=0, roomy, 8 do 
		add(tabley, i)
	end 
	
end 


function _update()
	walking()
end 
-- spr( n, x, y, [w,] [h,] [flip_x,] [flip_y] )

function _draw()
	popnums()
	cls(3)
	
	
	spr(12,96,72)
	palt(0, t)
	
	for i=0,80, 8 do 
		spr(6, i,8,1,2)
	end 
	
	-- plants 
	spr(1, 8, 104)
	
	palt(0,f)
	
	spr(9,72,16)
	palt(0,t)
	--water fall, placed +7, +4 from x and y
	--top
	spr(106,19,-3)
	spr(106,26,-3,1,1,t)
	
	--top near fall
	spr(106,19,4)
	spr(106,26,4,1,1,t)
	
	-- the fall bacground
	spr(103, 16,8,3,3)
	spr(107, 16, 32)
	spr(107, 32, 32,1,1,t)
	spr(34,24,32,1,1,f,t)
	
	-- the fall 
	spr(43,23,12,1,2)
	
	--do the sides 
	spr(71,88,8)
	spr(72,96,8,1,2)
	spr(22,88,16)
	spr(101,88,0,2,1)
	
	popflwr()
	pophouse()
	makepath()
	makebush()
	maketree()
	
	palt(0,f)
	palt(3, t)
	spr(64,x,y)
end 

function popflwr()
	
	--pink flwrs 
	pal(9,14)
	spr(18,24,48)
	pal()
	
	--orange flwrs
	spr(18,8,0)
	spr(18,80,40)
	
	-- yellow flowers 
	pal(9,10)
	pal(10,9)
	spr(18, 98, 98)
	pal() 
	
end

function pophouse()
	-- i like it offset by 2 
	spr(10,104,2,2,2)
	spr(25,112,-1) -- housex + 8, house y - 3
end 
 
 
function makebush()
	spr(51,24,64)
	for i = 32,48,8 do
		spr(52,i,64)
	end
	spr(51,56,64,1,1,t,f)
end  

function maketree()

	spr(41,64,56,2,2)
	
	-- reg tree  
	spr(4,32,104,2,2)
	spr(35,24,112)
end 

function makepath()
	for i=24,64,8 do
		spr(8,i,72) 
		spr(24, i,80)
		spr(8,i,88,1,1,f,t)
	end 
	
	for i = 32, 64, 8 do 
		spr(114,0,i)
		spr(115,8,i)
	end
	
	--130
	spr(130,0,24,2,1,f,t)
	spr(130,0,72,2,1)
	
	--path ends 
	spr(7,24-8,72)
	spr(112,24-8,80)
	spr(7,24-8,88,1,1,f,t)
	
	--rightside
	spr(7,64+8,72,1,1,t,f)
	spr(112,64+8,80,1,1,t,f)
	spr(7,64+8,88,1,1,t,t)
end 

function walking()
	
	if (btn(0)) then
			x -= 1 
	end
	
	if (btn(1)) then 
		x +=  1 
	end 
	
	if (btn(2)) then 
		y -= 1
	end 
	
	if (btn(3)) then 
		y += 1
	end 
	 
end 

